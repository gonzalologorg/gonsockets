<Window x:Class="SocketKeys.manage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="manage" Height="300" Width="150" ResizeMode="NoResize" ShowInTaskbar="False" Icon="icon.ico">
    <Grid Background="#FF2F2F34">
        <ListView x:Name="listView" Height="182" Margin="10,10,10,64" Width="122" Background="#FF28282A" SelectionChanged="listView_SelectionChanged" Foreground="#FFC8C8C8" BorderBrush="#FFBF8621" HorizontalContentAlignment="Stretch">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button Content="Remove" Margin="10,0,10,45" VerticalAlignment="Bottom" Height="18" Click="Button_Click" FontSize="10"/>
        <Button Content="Print" Margin="10,0,10,27" VerticalAlignment="Bottom" Height="18" Click="ButtonB_Click" FontSize="10"/>
        <Button Content="Close" Margin="10,0,10,10" VerticalAlignment="Bottom" Height="18" Click="ButtonC_Click" FontSize="10"/>

    </Grid>
</Window>
